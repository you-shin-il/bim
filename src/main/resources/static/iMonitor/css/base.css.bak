/* btnTab */
.mainState.step2 {display:none;}
.btnTab {position:fixed;left:50%;bottom:30px;margin-left:-100px;}
.btnTab a {display:inline-block;width:100px;height:25px;padding-top:55px;font-size:12px;color:#777;text-align:center;box-shadow:1px 1px 1px rgba(0,0,0,0.2)}
.btnTab a.on {box-shadow:1px 1px 4px rgba(0,0,0,0.3) inset}
.btnTab .btnResource{background:#3c444f url("../images/btn_resource.png") center 10px no-repeat;background-size:36px 36px;border-top-left-radius:10px;border-bottom-left-radius:10px;}
.btnTab .btnNonResource{background:#3c444f url("../images/btn_nonresource.png") center 10px no-repeat;background-size:36px 36px;border-top-right-radius:10px;border-bottom-right-radius:10px;}
.btnTab .btnResource.on{color:#fff;background:#273244 url("../images/btn_resource_on.png") center 10px no-repeat;background-size:36px 36px;}
.btnTab .btnNonResource.on{color:#fff;background:#273244 url("../images/btn_nonresource_on.png") center 10px no-repeat;background-size:36px 36px;}

/* Emergency notice */
.btnEmergencyOpen{position:fixed;bottom:30px;right:40px;display:inline-block;width:60px;height:60px;border-radius:60px;text-indent:-9999em;overflow:hidden;background:url("../images/ico_critical.png") center center no-repeat;}
.btnEmergencyOpen:hover{background:url("../images/ico_critical_on.png") center center no-repeat;}
.popEmergency{position:fixed;right:0;bottom:-220px;width:250px;background:rgba(0,0,0,0.4);border-top:0;}
.popEmergency h6{padding:0 10px;height:35px;line-height:35px;cursor:pointer;}
.popEmergency ul {margin:0 10px;padding:10px;height:200px;background:rgba(200,200,200,0.1);overflow-y:auto;}
.popEmergency ul li{height:23px;line-height:23px;}
.popEmergency ul li a{display:block;font-size:12px;color:#aaa; text-overflow:ellipsis; white-space:nowrap; overflow:hidden;}
.popEmergency ul li:first-child a {color:#fff;}
.popEmergency ul li a:before {content:"●";display:inline-block;font-size:10px;color:#c61212;margin-right:5px;}
.popEmergency ul li.critical a:before {color:#c61212;}
.popEmergency ul li.major a:before {color:#e88207;}
.popEmergency ul li.minor a:before {color:#dbcf07;}
.popEmergency ul li.normal a:before {color:#06ac56;}
.popEmergency ul li.stop a:before {color:#a69f98;}
.popEmergency .btnEmergencyClose{display:none;position:absolute; right:10px; top:10px;  width:15px; height:15px; border-radius:60px; text-indent:-9999em; overflow:hidden; background:url("../images/btn_popclose.png") center center no-repeat;}
.popEmergency.active{bottom:0;height:auto;}
.popEmergency.active h6{cursor:default;}
.popEmergency.active .btnEmergencyClose{display:block;}
