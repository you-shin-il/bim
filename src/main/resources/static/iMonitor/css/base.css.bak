/* Nav */
#nav h2{font-size:18px;font-weight:bold;}
#nav .gnb{position:absolute;top:20px;left:150px;}
#nav .gnb:after{display:block;content:'';clear:both;}
#nav .gnb li{float:left;position:relative;padding:0 15px;}
#nav .gnb li a{display:block;line-height:21px;}
#nav .gnb li.active a{color:#00a8c3;font-weight:bold;}
#nav .gnb li .sep{display:block;position:absolute;right:0;top:50%;margin-top:-6px;height:12px;border-right:1px solid #b1b1b1;}
#container .search{position:relative;margin-bottom:10px;text-align:right;}
#container .search .count{float:left;display:inline-block;padding:0 10px;vertical-align:middle;line-height:28px;font-weight:bold;}
#container .search .count em{color:#00a8c3;font-weight:bold;}
#container #monitorChart{margin:20px;}

/*H*/
h3{font-size:16px;font-weight:bold;line-height:1.5em;}

/*Form*/
select{padding:0 5px;height:24px;border:1px solid #aaa;}
input[type='submit']{height:24px;border:0px none;padding:0 10px;background:#2e3b61;color:#fff;font-size:0.9em;line-height:24px;cursor:pointer;}
input[type='text']{height:24px;border:1px solid #aaa;padding:0 5px;font-size:0.9em;line-height:24px;box-sizing:border-box;}
select:disabled,
textarea:disabled,
input[type="text"]:disabled{background:#ccc;}

/*Table*/
table{margin-top:8px;width:100%;border-top:2px solid #2e3b61;color:#333333;}
table tr{border-bottom:1px solid #c7c7c7;}
table th{padding:8px 6px;background:#dfe7ef;border-right:1px solid #c7c7c7;}
table td{padding:6px 6px;border-right:1px solid #c7c7c7;font-size:.9em;}
table.list{text-align:center;border-bottom:1px solid #c7c7c7;border-left:1px solid #c7c7c7;}
table.form th{text-align:left;}
table.form th em.required{display:inline-block;padding:3px;font-weight:bold;color:#e61e1e;line-height:1em;}
table.form td span{margin-right:20px;}

/*Paging*/

/*Btn*/
.btnHolder{margin:15px 0;}
.btn{display:inline-block;padding:0 12px;height:28px;font-size:0.9em;color:#444;line-height:26px;vertical-align:middle;border:1px solid #aaa;box-sizing:border-box;}
.btn_inline{display:inline-block;padding:0 10px;height:24px;font-size:0.9em;color:#444;line-height:22px;vertical-align:middle;border:1px solid #aaa;box-sizing:border-box;}
.btn_logout{color:#fff;border-color:#626777;}
.btn_save{color:#fff;background:#2e3b61;border-color:#2e3b61;}
.btn_period{}
.btn_period.active{color:#fff;background:#00a8c3;border-color:#00a8c3;}

.icon_status{display:inline-block;padding:0 6px;height:22px;font-size:12px;line-height:20px;border-radius:3px;vertical-align:middle;}
.icon_status.green{color:#fff;background:#65bc5b;}
.icon_status.red{color:#fff;background:#fd0303;}
.badge_status {display:inline-block;padding:0 6px;height:22px;font-size:12px;line-height:20px;border-radius:3px;vertical-align:middle;}
.badge_status.critical {background:#fd0303;}
.badge_status.major {background:#fd8301;}
.badge_status.minor{background:#dbcf07;}
.badge_status.normal{background:#65bc5b;}
.badge_status.stop{background:#a69f98;}

/*Datepicker*/
input.hasDatepicker{width:80px}
#ui-datepicker-div{display:none;width:200px;background:#fff;border:1px #ccc solid;box-shadow:2px 3px 6px #aaa;z-index:10 !important}
.ui-datepicker{position:relative}
.ui-datepicker table{margin:10px 0;width:100%;font-size:0.9em;border-top:0px none;}
.ui-datepicker table thead th,
.ui-datepicker table tbody td{height:inherit;padding:1px;font-weight:400;width:14.2857%;text-align:center}
.ui-datepicker table tbody td a{display:block;padding:6px 0;color:#4b4b4b;text-align:center}
.ui-datepicker table tbody td a.hover{background:#eee}
.ui-datepicker table tbody td span{color:#aaa}
.ui-datepicker table tbody td.date-holiday a{color:#eb3c3c}
.ui-datepicker table tbody tr td:first-child *{color:#eb3c3c}
.ui-datepicker table tbody tr td:last-child *{color:#4084d2}
.ui-datepicker table tbody td a.ui-state-active{background:#696a6c;color:#fff}
.ui-datepicker .ui-datepicker-header{position:relative}
.ui-datepicker .ui-datepicker-title{height:30px;font-size:0.9em;color:#fff;text-align:center;line-height:30px;background:#4395ce}
.ui-datepicker .ui-datepicker-prev,
.ui-datepicker .ui-datepicker-next{position:absolute;top:0;width:30px;height:30px;text-indent:-9999em;cursor:pointer}
.ui-datepicker .ui-datepicker-prev{left:0;background:url(../images/bg_arrowL.png) no-repeat center center;background-size:auto 50%}
.ui-datepicker .ui-datepicker-next{right:0;background:url(../images/bg_arrowR.png) no-repeat center center;background-size:auto 50%}
.btn_calendarOpen{display:inline-block;margin:0 2px 0 -2px;width:26px;height:26px;text-indent:-9999em;background:url(../images/icon_calendar.png) no-repeat center center;vertical-align:middle}
.hover .btn_calendarOpen,
.btn_calendarOpen.hover{background:url(../images/icon_calendar_on.png) no-repeat center center;background-size:auto 90%}

/* login */
fieldset{border:0px none;}
#loginForm{display:block;position:absolute;top:50%;left:0;right:0;margin-top:-250px;padding:40px 0;background:rgba(0,0,0,0.1);}
#loginForm h2{display:block;margin-bottom:20px;font-size:2.8em;color:#fff;text-align:center;}
#loginForm form{display:block;margin:0 auto;width:240px;font-size:0.9em;color:#666;text-align:center;}
#loginForm form #loginFormBox{margin-bottom:20px;border-radius:3px;overflow:hidden;border-right:1px solid #1d1c21;border-bottom:1px solid #1d1c21;}
#loginForm form #loginFormBox input[type='text'],
#loginForm form #loginFormBox input[type='password']{margin-bottom:1px;padding:0 12px;width:100%;height:40px;line-height:38px;border:0px none;}
#loginForm form #loginFormBox input[type='submit']{width:100%;height:36px;font-size:1.1em;font-weight:bold;color:#fff;letter-spacing:1px;text-align:center;background:rgba(0,0,0,0.1);border:0px none;}
#loginForm form #loginFormMent{margin:10px auto 0;width:240px;color:#a6aec7;}
#loginForm form #loginFormOption{margin:10px auto 0;width:240px;color:#a6aec7;}