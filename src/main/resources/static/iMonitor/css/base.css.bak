/*Layout*/
body{background:#1c2431 url("../images/img_back.png") 0 bottom no-repeat;background-size:cover;background-attachment:scroll;}
#wrap.main{}
#header{position:relative;height:50px;background:#1d1c21;}
#header h1{position:absolute;left:50%;top:11px;margin-left:-63px;}
#header .btnHeaderMenu{display:block;position:absolute;left:5px;top:0;width:50px; height:50px;text-indent:-9999em;overflow:hidden;background:url("../images/btn_menu.png") center center no-repeat;}
#header #headerMenu{position:fixed;left:-200px;top:50px;width:200px;height:100%;background:#1c2431;box-shadow:1px 1px 3px rgba(0,0,0,0.5);z-index:10;}
#header #headerMenu ul{padding:10px;}
#header #headerMenu ul li{height:35px;line-height:35px;border-bottom:1px solid #1d1c21;}
#header #headerMenu ul li a{font-size:12px;color:#aaa;}
#header .btnHeaderMenuClose{display:block;position:absolute;right:5px;top:5px; width:40px;height:40px;text-indent:-9999em;overflow:hidden;background:url("../images/btn_menuclose.png") center center no-repeat;}
#header .btnLogout{position:absolute;right:10px;top:0;display:inline-block;width:50px;height:50px;text-indent:-9999em;overflow:hidden;background:url("../images/btn_logout.png") center center no-repeat;}

#container{padding:10px;box-sizing:border-box;}
.dashboard{position:relative;}
.dashboard:after{display:block;content:"";clear:both;}
.dashboard .item{position:relative;float:left;}


